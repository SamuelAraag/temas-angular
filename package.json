{
  "name": "alternando-temas",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build-dark-theme": "sass src/tema-escuro.scss src/assets/temas/tema-escuro.css",
    "build-light-theme": "sass src/tema-claro.scss src/assets/temas/tema-claro.css",
    "build-css": "npm-run-all build-dark-theme build-light-theme",
    "start": "npm-run-all -p build-css ng-serve",
    "start:watch": "npm-run-all -p build-css --watch ng-serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "ng-serve": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/ssr": "^17.0.8",
    "bootstrap": "^5.3.3",
    "express": "^4.18.2",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.2.2"
  }
}
